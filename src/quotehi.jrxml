	<parameter name="iid" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT
     quote.`date` AS invoice_date,
     quote.`total` AS invoice_total,
     quote.`item_count` AS invoice_item_count,
     quote.`payed_amount` AS invoice_payed_amount,
     quote.`balance` AS invoice_balance,
     item.`barcode` AS item_barcode,
     quote_details.`unit_price` AS invoice_details_unit_price,
     sum(quote_details.`qty`)AS invoice_details_qty,
     quote_details.`discount` AS invoice_details_discount,
     payment_type.`type` AS payment_type_type,
     sum(quote_details.`sub_total`)AS invoice_details_sub_total,
     user.`user` AS user_user,
     item.`name` AS invoice_details_item_name,
     quote.`warranty` AS invoice_warranty,
     item.`serial_no` AS item_serial_no,
     SUBSTRING_INDEX(quote_details.`barcode`,' ',-2)AS warr,
     GROUP_CONCAT(SUBSTRING_INDEX(quote_details.`barcode`,' ',-1)SEPARATOR ',')AS invoice_barcode,
     customer.`first_name` AS customer_first_name,
     customer.`last_name` AS customerlast_name,
     customer.`tel` AS customer_tel
FROM
     `quote` quote INNER JOIN `quote_details` quote_details ON quote.`invoice_id` = quote_details.`invoice_invoice_id`
     INNER JOIN `item` item ON quote_details.`Item_id` = item.`id`
     INNER JOIN `payment_type` payment_type ON quote.`payment_type_id` = payment_type.`id`
     INNER JOIN `user` user ON quote.`user_id` = user.`id`
     INNER JOIN `customer` customer ON quote.`customer_id` = customer.`id`
WHERE
     quote.invoice_id = $P{iid}
GROUP BY
     item_id]]></queryString>

